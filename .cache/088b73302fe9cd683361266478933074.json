db.put({\n        \"_id\": `${generateUniqueId()}`,\n        \"text\": value\n    }).then(() => {\n        const input = $(\"#todo-text\");\n        input.value = \"\";\n        input.focus();\n\n        updateList();\n    })\n});\n\nwindow.onload = () => {\n    updateList();\n}\n"},"lineCount":null}},"hash":"6376d866940994b8c3dc973a59decd51","cacheData":{"env":{}}}